								jQuery('#emailto-button').click(function(){
									if( is_animating == false ){
										var url = jQuery(this).attr('url');
										var email_has_error = false;
										var email_to = jQuery('#emailto-to').val();
										email_to = jQuery.trim(email_to);
										var email_subject = jQuery('#emailto-subject').val();
										email_subject = jQuery.trim(email_subject);
										var email_message = jQuery('#emailto-message').val();
										email_message = jQuery.trim(email_message);
										var captcha1 = jQuery('#captcha1').html();
										captcha1 = jQuery.trim(captcha1);
										captcha1 = parseInt(captcha1);
										var captcha3 = jQuery('#captcha3').html();
										captcha3 = jQuery.trim(captcha3);
										captcha3 = parseInt(captcha3);
										var captcha_correct_answer = captcha1 + captcha3;
										var captcha_user_answer = jQuery('#emailto-captcha').val();
										captcha_user_answer = jQuery.trim(captcha_user_answer);
										captcha_user_answer = parseInt(captcha_user_answer);
										
										if( !checkEmail(email_to) ){
											jQuery('#emailto-to').css('border', '1px solid #B00000');
											email_has_error = true;
										}else{
											jQuery('#emailto-to').css('border', '1px solid #BFBFBF');
										}
										if( email_subject == '' ){
											jQuery('#emailto-subject').css('border', '1px solid #B00000');
											email_has_error = true;
										}else{
											jQuery('#emailto-subject').css('border', '1px solid #BFBFBF');
										}
										if( email_message == '' ){
											jQuery('#emailto-message').css('border', '1px solid #B00000');
											email_has_error = true;
										}else{
											jQuery('#emailto-message').css('border', '1px solid #BFBFBF');
										}
										if( captcha_user_answer != captcha_correct_answer){
											jQuery('#emailto-captcha').css('border', '1px solid #B00000');
											email_has_error = true;
										}else{
											jQuery('#emailto-captcha').css('border', '1px solid #BFBFBF');
										}
										
										if( email_has_error == true ){
											jQuery('#email-result').html('Invalid entries.');
										}else{
											jQuery('#email-result').html('');
											
											var post_data = jQuery('#emailto-form').serialize();
											
											is_animating = true;
											
											//set working status
											jQuery('#email-result').html('Sending...');
											
											jQuery.ajax({
												url: url,
												type: 'POST', // POST/GET
												dataType: 'html', // xml/json/html/script
												data: post_data,
												success: function(data) {
													if( data == 'failed' ){
														//set captchas
														jQuery('#captcha1').html(Math.floor(Math.random()*10));
														jQuery('#captcha3').html(Math.floor(Math.random()*10));
														jQuery('#emailto-captcha').val('');
														
														//set error message
														jQuery('#email-result').html('<span style="color: red;">Email not sent. Please try again later.</span>');
														
														is_animating = false;
														return false;
													}else if( data == '' || data == 'error' ){
														//set captchas
														jQuery('#captcha1').html(Math.floor(Math.random()*10));
														jQuery('#captcha3').html(Math.floor(Math.random()*10));
														jQuery('#emailto-captcha').val('');
														
														//set error message
														jQuery('#email-result').html('<span style="color: red;">Failed. Please try again later.</span>');
														
														is_animating = false;
														return false;
													}else if( data == 'success' ){
														//set captchas
														jQuery('#captcha1').html(Math.floor(Math.random()*10));
														jQuery('#captcha3').html(Math.floor(Math.random()*10));
														jQuery('#emailto-to').val('');
														jQuery('#emailto-captcha').val('');
														
														//set success message
														jQuery('#email-result').html('<span style="color: green;">Your email has been sent!</span>');
														
														is_animating = false;
														return false;
													}else{
														//set captchas
														jQuery('#captcha1').html(Math.floor(Math.random()*10));
														jQuery('#captcha3').html(Math.floor(Math.random()*10));
														jQuery('#emailto-captcha').val('');
														
														//set error message
														jQuery('#email-result').html('<span style="color: red;">Failed. Please try again later.</span>');
														
														is_animating = false;
														return false;
													}
												},
												
												error: function(xhr, ajaxOptions, thrownError){
													//alert(xhr.status);
													//alert(thrownError);
													
													//set captchas
													jQuery('#captcha1').html(Math.floor(Math.random()*10));
													jQuery('#captcha3').html(Math.floor(Math.random()*10));
													jQuery('#emailto-captcha').val('');
													
													//set error message
													jQuery('#email-result').html('<span style="color: red;">Failed. Please try again later.</span>');
													
													is_animating = false;
													return false;
												}
											});
										}
									}
								});
								
								//preloading block
								jQuery('.product-image').each(function(){
									jQuery(this).load(function(){
										//fadein image
										jQuery(this).fadeIn(1000);
									});
								});
							
								//product - mouseover event
								jQuery('.product-image-wrapper, .product-name-wrapper, .product-image').mouseover(function(){
									//get product id
									if( is_ajax_running == false && is_animating == false ){
										var product_id = jQuery(this).attr('product_id');
										product_id = parseInt(product_id) || 0;
										
										if( !jQuery('#product-item-wrapper-' + product_id).hasClass('active') ){
											//remove hover class from other products
											jQuery('.product-item-wrapper').removeClass('hover');
											
											//add hover class to hovered product
											jQuery('#product-item-wrapper-' + product_id).addClass('hover');
										}
									}
								});
								
								//product - mouseout event
								jQuery('.product-item-wrapper').mouseout(function(){
									if( is_ajax_running == false && is_animating == false ){
										//get product id
										var product_id = jQuery(this).attr('product_id');
										product_id = parseInt(product_id) || 0;
										
										if( !jQuery('#product-item-wrapper-' + product_id).hasClass('active') ){
											//remove hover class from the product
											jQuery('#product-item-wrapper-' + product_id).removeClass('hover');
										}
									}
								});
								
								//product - close event
								jQuery('.product-close-btn').click(function(){
									if( is_animating == false ){
										var product_id = jQuery(this).attr('product_id');
										product_id = parseInt(product_id) || 0;
										
										jQuery('#product-details-wrapper-' + product_id + ', #product-arrow-' + product_id).fadeOut({queue: false, duration: 500, easing: 'easeOutCubic', complete: function(){
											//hide product details wrapper
											jQuery('#product-details-wrapper-' + product_id).removeClass('active');
											
											//hide arrow
											jQuery('#product-arrow-' + product_id).removeClass('active');
											
											//remove activated item wrapper
											jQuery('#product-item-wrapper-' + product_id).removeClass('active');
											
											//resize fb page
											resizeWrapper();
											
											is_animating = false;
										}, step: function(){
											is_animating = true;
										}});
									}
								});
								
								//product - click event
								jQuery('.product-image-wrapper, .product-name-wrapper').click(function(){
									if( is_ajax_running == false && is_animating == false ){
										var product_id = jQuery(this).attr('product_id');
										product_id = parseInt(product_id) || 0;
										
										if( !jQuery('#product-item-wrapper-' + product_id).hasClass('active') ){
											//remove hovered products
											jQuery('.product-item-wrapper').removeClass('hover');
											
											//remove activated products
											jQuery('.product-item-wrapper').removeClass('active');
											
											//add class to product item wrapper
											jQuery('#product-item-wrapper-' + product_id).addClass('active');
											
											//fadeOut active products
											jQuery('.product-details-wrapper, .product-arrow').fadeOut({queue: false, duration: 400, easing: 'easeOutCubic', complete: function(){
												//resize fb page
												resizeWrapper();
											}, step: function(){
												is_animating = true;
											}});
											
											jQuery('#product-details-wrapper-' + product_id + ', #product-arrow-' + product_id).fadeIn({queue: false, duration: 500, easing: 'easeOutCubic', complete: function(){
												//show product details wrapper
												jQuery('#product-details-wrapper-' + product_id).addClass('active');
												
												//show arrow
												jQuery('#product-arrow-' + product_id).addClass('active');
												
												//resize fb page
												resizeWrapper();
												
												var scrollTop     		= jQuery(document).scrollTop(),
													elementOffset 		= jQuery('#product-details-wrapper-' + product_id).offset().top,
													distance_from_top	= (elementOffset - scrollTop);
												
												scrollToProductDetails(elementOffset);
												
												is_animating = false;
											}, step: function(){
												is_animating = true;
											}});
										}else{
											//remove hovered products
											jQuery('.product-item-wrapper').removeClass('hover');
											
											//remove activated products
											jQuery('.product-item-wrapper').removeClass('active');
											
											//remove activated item wrapper
											jQuery('#product-item-wrapper-' + product_id).removeClass('active');
											
											jQuery('#product-details-wrapper-' + product_id + ', #product-arrow-' + product_id).fadeOut({queue: false, duration: 500, easing: 'easeOutCubic', complete: function(){
												//show product details wrapper
												jQuery('#product-details-wrapper-' + product_id).removeClass('active');
												
												//show arrow
												jQuery('#product-arrow-' + product_id).removeClass('active');
												
												//resize fb page
												resizeWrapper();
												
												is_animating = false;
											}, step: function(){
												is_animating = true;
											}});
										}
									}
								});
							});
						</script>
															
			