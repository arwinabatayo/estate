														jQuery('#captcha1').html(Math.floor(Math.random()*10));
														jQuery('#captcha3').html(Math.floor(Math.random()*10));
														jQuery('#emailto-captcha').val('');
														
														//set error message
														jQuery('#email-result').html('<span style="color: red;">Failed. Please try again later.</span>');
														
														is_animating = false;
														return false;
													}
												},
												
												error: function(xhr, ajaxOptions, thrownError){
