							jQuery(document).ready(function(){
								is_animating = false;
								is_ajax_running = false;
								
								//check if there's only one product
								if( jQuery('.product-item-wrapper').length == 1 ){
									jQuery('.product-item-wrapper:first').addClass('has-one-product');
								}
								
								//for product facebook share
								jQuery('.product-facebook-icon').click(function(){
									if( is_animating == false ){
